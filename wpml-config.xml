<wpml-config>
    <custom-fields>
        <custom-field action="ignore">rev_slider</custom-field>
        <custom-field action="ignore">header_back</custom-field>
        <custom-field action="ignore">footer_back</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">dishes</custom-type>
        <custom-type translate="1">gallery</custom-type>
        <custom-type translate="1">offers</custom-type>
        <custom-type translate="1">recipe_gallery</custom-type>
    </custom-types>
    <taxonomies>
        <taxonomy translate="1">dishes_tax</taxonomy>
        <taxonomy translate="1">gallery_tax</taxonomy>
    </taxonomies>    
    <admin-texts>
        <key name="cuisinier_options">
            <key name="logo_text" />
            <key name="email_contact" />
            <key name="error_title" />
            <key name="error_message" />
            <key name="header_text" />
            <key name="copyright_message" />
            <key name="footer_text" />
            <key name="title_contact" />
            <key name="contact_address" />
            <key name="contact_phone" />
            <key name="contact_fax" />
        </key>
    </admin-texts>
</wpml-config>